{
  "name": "@kahirokunn/ts-curry",
  "version": "1.0.3",
  "description": "type safe curry.",
  "keywords": [
    "typescript",
    "curry",
    "ts-curry",
    "type-safe-ts-curry"
  ],
  "bugs": {
    "url": "https://github.com/kahirokunn/ts-curry/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kahirokunn/ts-curry.git"
  },
  "license": "MIT",
  "author": "kahirokunn",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.js",
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "build": "npm run cleanup && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "cleanup": "rm -rf lib esm",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --max-warnings=0",
    "lint:fix": "npm run prettier && npm run eslint --fix",
    "prepublishOnly": "npm run build",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx,json}' --write",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.json": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{yml,yaml}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.24.3",
    "@babel/preset-env": "7.24.3",
    "@babel/preset-typescript": "7.24.1",
    "@types/jest": "26.0.20",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "27.0.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "sort-package-json": "1.48.1",
    "ts-node": "9.1.1",
    "type-fest": "0.20.2",
    "typescript": "4.1.3"
  }
}
